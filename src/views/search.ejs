                <option value="3">เบอร์โทร</option>
                <option value="4">หมายเลขเครื่อง</option>
              </select>
            </div>
            <div class="my-input-container">
              <label for="input-search" class="mb-2">ช่องค้นหาเพื่อจัดการ</label>
              <div class="my-input-group">
                <input type="text" class="form-control" placeholder="ค้นหา">
                <button type="submit" class="btn btn-lg btn-primary">ค้นหา</button>
              </div>
            </div>
          </form>
        </div>
      </div>
      <%- include('./components/footer.ejs') %>
    </div>
  </div>
  
  
  <script src="/js/left-menu-expand.js"></script>

